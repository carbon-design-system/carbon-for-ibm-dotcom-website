{
  "name": "carbon-for-ibm-dotcom-website",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "gatsby develop -H 0.0.0.0",
    "dev:clean": "yarn clean && yarn dev",
    "build": "gatsby build --prefix-paths",
    "build:clean": "yarn clean && gatsby build --prefix-paths",
    "format": "prettier --write 'src/**/*.{css,scss,js,json,html,yaml,md,mdx}'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "build:analyze": "ANALYZE=true yarn build",
    "build-wiki": "node node_modules/wiki-helpers/index.js rootPath=./wiki buildPath=./wiki-build flattenDir=true"
  },
  "dependencies": {
    "@carbon/icons-react": "^10.27.0",
    "beacon-for-ibm-dotcom": "^0.2.7",
    "gatsby": "^2.19.17",
    "gatsby-theme-carbon": "1.30.2",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "wiki-helpers": "^1.2.4"
  },
  "devDependencies": {
    "@actions/core": "1.2.6",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-classes": "^7.12.13",
    "babel-preset-gatsby": "^0.5.2",
    "dotenv": "8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,json,html,yaml,md,mdx}": [
      "prettier --write"
    ]
  }
}
